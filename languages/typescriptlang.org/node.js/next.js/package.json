{
	"name": "@reverse-proxy/node-next",
	"version": "0.0.1",
	"private": true,
	"description": "Reverse Proxy",
	"keywords": [
		"reverse",
		"proxy"
	],
	"homepage": "https://github.com/hieudoanm/reverse-proxy#readme",
	"bugs": {
		"url": "https://github.com/hieudoanm/reverse-proxy/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/hieudoanm/reverse-proxy.git"
	},
	"license": "GPL-3.0",
	"author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
	"packageManager": "pnpm@10.18.3",
	"scripts": {
		"predev": "kill-port 3000",
		"dev": "next dev --turbopack",
		"prebuild": "rimraf .next docs",
		"build": "next build --turbopack",
		"postbuild": "touch ./docs/.nojekyll",
		"build:tauri": "tauri build",
		"build:android": "rm -rf mobile/android && cap add android && cap sync android",
		"build:ios": "rm -rf mobile/ios && cap add ios && cap sync ios",
		"build:mobile": "pnpm run build:android && pnpm run build:ios",
		"prestart": "kill-port 3000",
		"start": "next start",
		"test": "jest",
		"format": "prettier --cache --write .",
		"lint": "eslint",
		"prepare": "husky"
	},
	"dependencies": {
		"@capacitor/android": "7.4.3",
		"@capacitor/ios": "7.4.3",
		"next": "15.5.6",
		"pino": "10.0.0",
		"react": "19.2.0",
		"react-dom": "19.2.0"
	},
	"devDependencies": {
		"@capacitor/cli": "7.4.3",
		"@eslint/eslintrc": "3.3.1",
		"@tailwindcss/postcss": "4.1.14",
		"@tauri-apps/cli": "2.8.4",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.0",
		"@types/jest": "30.0.0",
		"@types/node": "24.8.1",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"daisyui": "5.3.7",
		"eslint": "9.37.0",
		"eslint-config-next": "15.5.6",
		"husky": "9.1.7",
		"jest": "30.2.0",
		"jest-environment-jsdom": "30.2.0",
		"kill-port": "2.0.1",
		"prettier": "3.6.2",
		"prettier-plugin-tailwindcss": "0.7.0",
		"rimraf": "6.0.1",
		"tailwindcss": "4.1.14",
		"ts-jest": "29.4.5",
		"typescript": "5.9.3"
	}
}
