  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "predev": "kill-port 3000",
    "dev": "next dev --turbopack",
    "prebuild": "rimraf .next",
    "build": "next build --turbopack",
    "build:tauri": "tauri build",
    "build:android": "rm -rf mobile/android && cap add android && cap sync android",
    "build:ios": "rm -rf mobile/ios && cap add ios && cap sync ios",
    "build:mobile": "pnpm run build:android && pnpm run build:ios",
    "prestart": "kill-port 3000",
    "start": "next start",
    "test": "jest",
    "format": "prettier --cache --write .",
    "lint": "eslint"
  },
  "dependencies": {
    "@capacitor/android": "7.4.4",
    "@capacitor/ios": "7.4.4",
    "next": "16.0.1",
    "pino": "10.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.4",
    "@eslint/eslintrc": "3.3.1",
    "@tailwindcss/postcss": "4.1.16",
    "@tauri-apps/cli": "2.9.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "daisyui": "5.3.10",
    "eslint": "9.38.0",
    "eslint-config-next": "16.0.1",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "kill-port": "2.0.1",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "rimraf": "6.0.1",
    "tailwindcss": "4.1.16",
    "ts-jest": "29.4.5",
    "typescript": "5.9.3"
  }
}
