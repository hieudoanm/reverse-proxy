{
  "name": "@reverse-proxy/node-next",
  "version": "0.0.1",
  "private": true,
  "description": "Reverse Proxy",
  "keywords": [
    "reverse",
    "proxy"
  ],
  "homepage": "https://github.com/hieudoanm/reverse-proxy#readme",
  "bugs": {
    "url": "https://github.com/hieudoanm/reverse-proxy/issues"
  },
  "repository": {
    "type": "git",
